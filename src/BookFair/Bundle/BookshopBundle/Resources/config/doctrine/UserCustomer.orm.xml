<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="BookFair\Bundle\BookshopBundle\Entity\UserCustomer" table="user_customer">
    <unique-constraints>
      <unique-constraint name="email" columns="username"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="username" type="string" column="username" length="30" nullable="true"/>
    <field name="password" type="string" column="password" length="60" nullable="true"/>
  </entity>
</doctrine-mapping>
