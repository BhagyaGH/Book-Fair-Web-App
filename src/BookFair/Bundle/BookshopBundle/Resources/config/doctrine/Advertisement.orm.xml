<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="BookFair\Bundle\BookshopBundle\Entity\Advertisement" table="advertisement">
    <id name="adId" type="string" column="ad_id" length="3">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="image" type="blob" column="image" nullable="false"/>
    <field name="validTill" type="date" column="valid_till" nullable="false"/>
  </entity>
</doctrine-mapping>
